    "ignore-styles": "^5.0.1",
    "jsdom": "^11.7.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "prepublish": "^1.6.3",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "scripts": {
    "clean": "rimraf dist es",
    "build": "npm run clean && npm run build:webpack && npm run build:es && npm run build:es-css",
    "build:es": "cross-env babel src -d es -D -s --minified --compact true --ignore '**/__test__/*'",
    "build:es-css": "postcss 'src/**/*.css' -d es -m -v --base src",
    "build:webpack": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "dev": "rollup --config=config/rollup.config.js -w",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test mocha --require babel-register --require ignore-styles --require config/test-setup.js src/**/*Test.js",
    "lint": "eslint src",
    "lintdocs": "eslint docs/src",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "check": "npm run lint && npm run flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "es"
  ],
  "license": "MIT",
  "resolutions": {
    "immutable": "4.0.0-rc.9"
  }
}
